<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <stop-on-errors>false</stop-on-errors>
        <date-format>MM/dd/yyyy HH:mm</date-format>
    </configuration>

    <mapping>
        <class-a>com.thegutuproject.pizzashop.domain.OrderEntry</class-a>
        <class-b>com.thegutuproject.pizzashop.model.OrderEntryModel</class-b>
    </mapping>
    <mapping>
        <class-a>com.thegutuproject.pizzashop.domain.OrderLog</class-a>
        <class-b>com.thegutuproject.pizzashop.model.OrderLogModel</class-b>
    </mapping>
    <mapping>
        <class-a>com.thegutuproject.pizzashop.db.model.OrderEntryDb</class-a>
        <class-b>com.thegutuproject.pizzashop.domain.OrderEntry</class-b>
    </mapping>
    <mapping>
        <class-a>com.thegutuproject.pizzashop.db.model.OrderLogDb</class-a>
        <class-b>com.thegutuproject.pizzashop.domain.OrderLog</class-b>
    </mapping>

</mappings>